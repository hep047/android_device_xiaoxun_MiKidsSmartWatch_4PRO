/system       ext4     /dev/block/mmcblk0p21       flags=display=system
/vendor       ext4     /dev/block/mmcblk0p22       flags=display=vendor
/cache        ext4     /dev/block/mmcblk0p24        flags=display=cache
/data         ext4     /dev/block/mmcblk0p34     flags=display=data
/external_sd       vfat     /dev/block/mmcblk1p1                       flags=display=sdcard;storage;wipeingui;removable
/boot         emmc     /dev/block/mmcblk0p20         flags=display=boot
/recovery     emmc     /dev/block/mmcblk0p25     flags=display=recovery
/misc         emmc     /dev/block/mmcblk0p13         flags=display=misc
